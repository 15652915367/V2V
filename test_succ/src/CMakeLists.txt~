# v2v
set(
    src_SRCS
	CommonUtility.cc

    main.cc
	getGPS.cc
	HvData.cc
	V2V.cpp
	Scenes.cc
	RvData.cc
	RvMap.cc
	Sock_2Android.cpp
	CANbus.cpp
	readCSV/readCSV.cc
	CodecCrypt.cc
	AesUtility.cc
)

add_executable(v2v ${src_SRCS})
target_link_libraries(
    v2v 
    ${STATIC_LIBRARIES_INPUT_PATH}/libv2v_asn.a
    ${STATIC_LIBRARIES_INPUT_PATH}/libmuduo_base.a
    ${STATIC_LIBRARIES_INPUT_PATH}/libmuduo_net.a
    pthread
    rt
    #crypto
	/usr/local/arm/openssl/lib/libcrypto.a
	dl
	m
	#z
	/home/idiot/Documents/zlib-1.2.11/libz.a
)
install(TARGETS v2v DESTINATION bin)




